<keymap version="1" name="Dan's keymaps" parent="$default">
  <action id="ActivateFavoritesToolWindow" />
  <action id="ActivateHierarchyToolWindow">
    <keyboard-shortcut first-keystroke="alt 8" />
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateMavenProjectsToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
  </action>
  <action id="ActivateProblemsToolWindow">
    <keyboard-shortcut first-keystroke="alt 4" />
  </action>
  <action id="ActivateRunToolWindow" />
  <action id="ActivateStructureToolWindow" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="EditorMoveToPageBottom" />
  <action id="EditorMoveToPageTop" />
  <action id="EditorScrollToCenter" />
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="ctrl f12" />
    <keyboard-shortcut first-keystroke="ctrl o" />
  </action>
  <action id="HideAllWindows">
    <keyboard-shortcut first-keystroke="shift ctrl f12" />
    <keyboard-shortcut first-keystroke="ctrl m" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="ctrl page_down" />
  </action>
  <action id="OverrideMethods" />
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="ctrl page_up" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="ShowPopupMenu">
    <keyboard-shortcut first-keystroke="context_menu" />
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="Vcs.ShowMessageHistory">
    <keyboard-shortcut first-keystroke="ctrl e" />
  </action>
</keymap>